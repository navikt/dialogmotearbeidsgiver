{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "lib": ["es2019", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "outDir": "/",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./js/*"]
    },
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "esModuleInterop": true
  }
}
